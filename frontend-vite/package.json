    "autoprefixer": "^10.4.14",
    "eslint": "^8.41.0",
    "eslint-plugin-vue": "^9.14.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2",
    "typescript": "~5.0.4",
    "vite": "^4.3.9",
    "vue-tsc": "^1.6.5"
