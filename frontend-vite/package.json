    "primevue": "^3.29.1",
    "sass": "^1.62.1",
    "vue": "^3.2.36",
    "vue-clipboard3": "^2.0.0",
    "vue-feather": "^2.0.0",
    "vue-router": "^4.2.2",
    "vue3-mq": "^3.1.3",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/debounce": "^1.2.1",
